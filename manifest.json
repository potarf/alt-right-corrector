{
"manifest_version": 2,
    "name": "Alt-right Corrector",
    "description": "Corrects the phrase 'alt-right' to 'white supremacist'.",
    "version": "1.0",
    "content_scripts": [
        {
            "matches": [
                "*://*/*"
            ],
            "js": [
                "content.js"
            ],
            "run_at": "document_end"
        }
    ],
    "icons": { "16": "icon16.png",
            "48": "icon48.png",
           "128": "icon128.png" }
}
